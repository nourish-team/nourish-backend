{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node src/seed.ts"
  },
  "overrides": {
    "semver": "^7.5.3"
  },
  "scripts": {
    "start": "node build/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p .",
    "dev:startdb": "npm run db:dockerup && npm run prisma:migrate && npm run prisma:seed",
    "db:dockerup": "echo \"🐳✨ Starting docker compose...✨🐳\" && docker compose -f docker/docker-compose-db-dev.yml up -d",
    "db:dockerdown": "echo \"🐋 Stopping docker compose... 🐋\" && docker compose -f docker/docker-compose-db-dev.yml down",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "echo \"🔄️ Running migrations...🔄️\" && npx prisma migrate dev",
    "prisma:seed": "npx prisma db seed",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "husky install",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^2.0.22",
    "pg": "^8.11.0",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
